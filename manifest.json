{
  "manifest_version": 3,
  "name": "YouTube Addiction Rehab",
  "version": "1.0",
  "permissions": ["tabs", "storage"],
  "options_page": "options/options.html",
  "background": {
      "service_worker": "scripts/background.js"
  },
  "action": {},
  "content_scripts": [
      {
          "matches": ["*://www.youtube.com/*"],
          "js": ["scripts/content-script.js"]
      }
  ]
}